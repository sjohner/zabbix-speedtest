zabbix_export:
  version: '7.4'
  template_groups:
    - uuid: 7df96b18c230490a9a0a9e2307226338
      name: Templates
  templates:
    - uuid: 9daa55f388e94272af1cacad9807e847
      template: Speedtest
      name: Speedtest
      groups:
        - name: Templates
      items:
        - uuid: 0235cd698a344267bd700cde79e267a3
          name: Download
          type: TRAP
          key: speedtest.download
          history: 90d
          value_type: FLOAT
          units: bps
          preprocessing:
            - type: MULTIPLIER
              parameters:
                - '8'
          tags:
            - tag: Application
              value: Speedtest
          triggers:
            - uuid: 878dfcb25d0b43cea865073c68a95726
              expression: 'last(/Speedtest/speedtest.download,#1)<125000000'
              name: 'Download Speed Degraded'
              priority: WARNING
              description: 'Create problem if download speed drops below 100Mbps'
        - uuid: e8c3affb49dd4315b305f934874f006f
          name: ISP
          type: TRAP
          key: speedtest.isp
          history: 90d
          value_type: TEXT
          tags:
            - tag: Application
              value: Speedtest
        - uuid: a4f0bb23f9be48408794edf44be18e62
          name: 'Ping Jitter'
          type: TRAP
          key: speedtest.ping.jitter
          history: 90d
          value_type: FLOAT
          tags:
            - tag: Application
              value: Speedtest
        - uuid: 39c21c42893a4467bdb9540c3b253803
          name: 'Ping Latency'
          type: TRAP
          key: speedtest.ping.latency
          history: 90d
          value_type: FLOAT
          units: ms
          tags:
            - tag: Application
              value: Speedtest
        - uuid: 176bf5b911394e2f82694f80782e2192
          name: 'Server City'
          type: TRAP
          key: speedtest.srv.city
          history: 90d
          value_type: TEXT
          tags:
            - tag: Application
              value: Speedtest
        - uuid: 35ba8a35fbf742e4b3abe7bd13e27d11
          name: 'Server Name'
          type: TRAP
          key: speedtest.srv.name
          history: 90d
          value_type: TEXT
          tags:
            - tag: Application
              value: Speedtest
        - uuid: 3331a707a6ed423c80d779665a6bbf32
          name: Upload
          type: TRAP
          key: speedtest.upload
          history: 90d
          value_type: FLOAT
          units: Bps
          preprocessing:
            - type: MULTIPLIER
              parameters:
                - '8'
          tags:
            - tag: Application
              value: Speedtest
          triggers:
            - uuid: 822cca0fb44a4d02843cbdcc4762e776
              expression: 'last(/Speedtest/speedtest.upload,#1)<50000000'
              name: 'Upload Speed Degraded'
              priority: WARNING
              description: 'Create problem if download speed drops below 100Mbps'
              manual_close: 'YES'
        - uuid: acacbbd035c949c789a1d2d2a9b9f258
          name: 'WAN IP'
          type: TRAP
          key: speedtest.wan.ip
          history: 90d
          value_type: TEXT
          tags:
            - tag: Application
              value: Speedtest
      dashboards:
        - uuid: 387785bcebee495e915a44c5075b5019
          name: 'Speedtest Status'
          pages:
            - widgets:
                - type: svggraph
                  name: 'Speedtest Download vs Upload'
                  width: '60'
                  height: '6'
                  fields:
                    - type: INTEGER
                      name: ds.0.color_palette
                      value: '0'
                    - type: STRING
                      name: ds.0.items.0
                      value: Download
                    - type: STRING
                      name: ds.0.items.1
                      value: Upload
                    - type: STRING
                      name: reference
                      value: UUIHS
                    - type: INTEGER
                      name: righty
                      value: '0'
                - type: svggraph
                  name: 'Speedtest Ping Latency vs Jitter'
                  'y': '6'
                  width: '60'
                  height: '6'
                  fields:
                    - type: INTEGER
                      name: ds.0.color_palette
                      value: '0'
                    - type: STRING
                      name: ds.0.items.0
                      value: 'Ping Jitter'
                    - type: STRING
                      name: ds.0.items.1
                      value: 'Ping Latency'
                    - type: STRING
                      name: reference
                      value: TZQAG
                    - type: INTEGER
                      name: righty
                      value: '0'
                - type: problems
                  name: Problems
                  x: '60'
                  width: '12'
                  height: '6'
                  fields:
                    - type: STRING
                      name: reference
                      value: HOTNL
                    - type: INTEGER
                      name: severities.0
                      value: '0'
                    - type: INTEGER
                      name: severities.1
                      value: '1'
                    - type: INTEGER
                      name: severities.2
                      value: '2'
                    - type: INTEGER
                      name: severities.3
                      value: '3'
                    - type: INTEGER
                      name: severities.4
                      value: '4'
                    - type: INTEGER
                      name: severities.5
                      value: '5'
                    - type: INTEGER
                      name: tags.0.operator
                      value: '0'
                    - type: STRING
                      name: tags.0.tag
                      value: Application
                    - type: STRING
                      name: tags.0.value
                      value: Speedtest
                - type: item
                  name: 'Server Location'
                  x: '60'
                  'y': '6'
                  width: '12'
                  height: '3'
                  fields:
                    - type: ITEM
                      name: itemid.0
                      value:
                        host: Speedtest
                        key: speedtest.srv.city
                    - type: INTEGER
                      name: show.0
                      value: '2'
                    - type: INTEGER
                      name: show.1
                      value: '4'
                - type: item
                  name: 'Speedtest Server'
                  x: '60'
                  'y': '9'
                  width: '12'
                  height: '3'
                  fields:
                    - type: ITEM
                      name: itemid.0
                      value:
                        host: Speedtest
                        key: speedtest.srv.name
                    - type: INTEGER
                      name: show.0
                      value: '2'
                    - type: INTEGER
                      name: show.1
                      value: '4'
  graphs:
    - uuid: 8934ca31c4a14171a67575556ac2cec9
      name: 'Speedtest Upload vs Download'
      show_work_period: 'NO'
      show_triggers: 'NO'
      graph_items:
        - drawtype: BOLD_LINE
          color: 1A7C11
          calc_fnc: MAX
          item:
            host: Speedtest
            key: speedtest.download
        - sortorder: '1'
          drawtype: BOLD_LINE
          color: '000066'
          calc_fnc: MAX
          item:
            host: Speedtest
            key: speedtest.upload
